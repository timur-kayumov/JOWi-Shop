{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "sharedGlobals": [
      "{workspaceRoot}/**/.env.*local"
    ],
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ]
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/.next/**",
        "!{projectRoot}/.next/cache/**",
        "{projectRoot}/dist/**",
        "{projectRoot}/build/**",
        "{projectRoot}/out/**"
      ],
      "cache": true
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "cache": true
    },
    "type-check": {
      "dependsOn": [
        "^type-check"
      ],
      "cache": true
    },
    "clean": {
      "cache": false
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:migrate": {
      "cache": false
    },
    "db:studio": {
      "cache": false
    },
    "db:seed": {
      "cache": false
    }
  }
}